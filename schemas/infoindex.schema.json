{
  "$defs": {
    "SubdirStr": {
      "enum": [
        "emscripten-wasm32",
        "freebsd-64",
        "linux-32",
        "linux-64",
        "linux-armv6l",
        "linux-armv7l",
        "linux-ppc64le",
        "linux-s390x",
        "osx-32",
        "osx-64",
        "osx-arm64",
        "wasi-wasm32",
        "win-32",
        "win-64",
        "win-arm64",
        "zos-z",
        "noarch"
      ],
      "title": "SubdirStr",
      "type": "string"
    }
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "arch": {
      "minLength": 1,
      "title": "Arch",
      "type": "string"
    },
    "build": {
      "maxLength": 64,
      "minLength": 1,
      "pattern": "^[a-zA-Z0-9_\\.+]+$",
      "title": "Build",
      "type": "string"
    },
    "build_number": {
      "minimum": 0,
      "title": "Build Number",
      "type": "integer"
    },
    "constrains": {
      "default": [],
      "items": {
        "minLength": 1,
        "pattern": "(^([a-z0-9]+|(_[a-z0-9]+))[._-]?([a-z0-9]+(\\.|-|_|$))*$)\\s+(([0-9a-z<>=!\\.\\*]+)|([0-9a-z<>=!\\.\\*]+)?\\s+(^[a-zA-Z0-9_\\.+*]+$))?",
        "type": "string"
      },
      "title": "Constrains",
      "type": "array"
    },
    "depends": {
      "default": [],
      "items": {
        "minLength": 1,
        "pattern": "(^([a-z0-9]+|(_[a-z0-9]+))[._-]?([a-z0-9]+(\\.|-|_|$))*$)\\s+(([0-9a-z<>=!\\.\\*]+)|([0-9a-z<>=!\\.\\*]+)?\\s+(^[a-zA-Z0-9_\\.+*]+$))?",
        "type": "string"
      },
      "title": "Depends",
      "type": "array"
    },
    "license": {
      "minLength": 1,
      "title": "License",
      "type": "string"
    },
    "name": {
      "maxLength": 64,
      "minLength": 1,
      "pattern": "^([a-z0-9]+|(_[a-z0-9]+))[._-]?([a-z0-9]+(\\.|-|_|$))*$",
      "title": "Name",
      "type": "string"
    },
    "platform": {
      "minLength": 1,
      "title": "Platform",
      "type": "string"
    },
    "subdir": {
      "$ref": "#/$defs/SubdirStr"
    },
    "timestamp": {
      "default": 0,
      "exclusiveMinimum": 0,
      "title": "Timestamp",
      "type": "integer"
    },
    "version": {
      "maxLength": 64,
      "minLength": 1,
      "pattern": "([0-9]!)?[0-9a-z\\._]+",
      "title": "Version",
      "type": "string"
    }
  },
  "required": [
    "arch",
    "build",
    "build_number",
    "license",
    "name",
    "platform",
    "subdir",
    "version"
  ],
  "title": "InfoIndex",
  "type": "object"
}
