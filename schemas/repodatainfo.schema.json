{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "Repodata's `*.info.json` files",
  "properties": {
    "blake2_256": {
      "maxLength": 64,
      "minLength": 64,
      "pattern": "[a-fA-F0-9]{64}",
      "title": "Blake2 256",
      "type": "string"
    },
    "blake2_256_nominal": {
      "maxLength": 64,
      "minLength": 64,
      "pattern": "[a-fA-F0-9]{64}",
      "title": "Blake2 256 Nominal",
      "type": "string"
    },
    "cache_control": {
      "title": "Cache Control",
      "type": "string"
    },
    "etag": {
      "title": "Etag",
      "type": "string"
    },
    "mod": {
      "title": "Mod",
      "type": "string"
    },
    "mtime_ns": {
      "title": "Mtime Ns",
      "type": "integer"
    },
    "refresh_ns": {
      "title": "Refresh Ns",
      "type": "integer"
    },
    "size": {
      "title": "Size",
      "type": "integer"
    },
    "url": {
      "title": "Url",
      "type": "string"
    }
  },
  "required": [
    "blake2_256_nominal",
    "blake2_256",
    "cache_control",
    "etag",
    "mod",
    "mtime_ns",
    "refresh_ns",
    "size",
    "url"
  ],
  "title": "RepodataInfo",
  "type": "object"
}
